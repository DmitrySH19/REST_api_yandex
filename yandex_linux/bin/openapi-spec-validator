#!/home/ezrial321/REST_api_yandex/yandex_linux/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'openapi-spec-validator','console_scripts','openapi-spec-validator'
__requires__ = 'openapi-spec-validator'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('openapi-spec-validator', 'console_scripts', 'openapi-spec-validator')()
    )
